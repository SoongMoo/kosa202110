<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="
	   http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans.xsd
	   http://www.springframework.org/schema/context
	   http://www.springframework.org/schema/context/spring-context.xsd"
>
	<!-- 의존 객체 자동주입 -->
	<context:annotation-config />
	<bean id="memberDao"             
					class="spring.dependencyTest5.DTO.MemberDao"                 />
	<bean id="printer"               
					class="spring.dependencyTest5.service.MemberPrinter"         />
	<bean id="memberListPrinter"     
					class="spring.dependencyTest5.service.MemberListPrinter"     />
	<bean id="memberInfoPrinter"     	
					class="spring.dependencyTest5.service.MemberInfoPrinter"     />
	<bean id="changePasswordService" 
					class="spring.dependencyTest5.service.ChangePasswordService" />
	<bean id="memberRegisterService" 
					class="spring.dependencyTest5.service.MemberRegisterService" />

</beans>






